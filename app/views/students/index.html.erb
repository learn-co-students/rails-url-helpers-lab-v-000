<div>
  <% @students.each do |student| %>
    <div><%= student.to_s %></div>
    <div><a href='<%= "/students/#{student.id}" %>'><%= student.to_s %></a></div>
    <div><%= link_to student.to_s, "/students/#{student.id}" %></div>
     <div><%= link_to student.to_s, student_path(student) %></div>
  <% end %>


</div>